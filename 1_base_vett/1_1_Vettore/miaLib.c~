#include <iostream>
#include "miaLib.h"
using namespace std;

void leggiVettore(int vettore[], const int n){
	
	for(int i=0; i<n; i++){
		cout<<"inserisci elemento nel vetore in pos "<<i <<" : ";
		cin>>vettore[i];
	}

}

void stampaVettore(int vettore[], const int n){

	for(int i=0; i<n; i++){
		cout<<"stampo vetore in pos "<<i <<" : "<< vettore[i]<<"\n";;
	}

}
